<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="StateType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PENDING"/>
      <xs:enumeration value="READY"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="state" type="StateType" />
</xs:schema>